{
   "manifest_version": 2,
   "name": "#name#",
   "permissions": [ "#url#/*" ],
   "version": "1",
   "web_accessible_resources": [ "html_code.js" ],

   "description": "#description#",
   "content_scripts": [ {
      "js": [ "jquery.js", "main.js" ],
      "matches": [ "#url#/*" ]
   } ],
   "icons": {
      "128": "icon128.png",
      "16": "icon16.png",
      "48": "icon48.png"
   }
}

